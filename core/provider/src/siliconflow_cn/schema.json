{
  "provider_config": {
    "api_key": {
      "name": "API 密钥",
      "type": "sensitive",
      "default": null,
      "hint": "硅基流动 API 密钥，在 https://cloud.siliconflow.cn/me/account/ak 获取"
    }
  },
  "model_config": {
    "llm": {
      "timeout": {
        "name": "超时",
        "type": "integer",
        "default": 120,
        "min": 1,
        "hint": "超时秒数"
      },
      "temperature": {
        "name": "温度",
        "type": "float",
        "default": null,
        "min": 0,
        "max": 2,
        "hint": "模型温度"
      }
    },
    "image": {
      "image_size": {
        "name": "图片尺寸",
        "type": "string",
        "default": "1024x1024",
        "hint": "图片尺寸"
      },
      "num_inference_steps": {
        "name": "推理步数",
        "type": "integer",
        "default": 20,
        "hint": "推理步数"
      }
    },
    "tts": {
      "voice_name": {
        "name": "声音ID",
        "type": "string",
        "default": "speech:xxxx:xxxx:xxxxxx",
        "hint": "声音ID，可以在 https://docs.siliconflow.cn/cn/api-reference/audio/upload-voice 上传参考音频获取声音名称"
      }
    },
    "stt": {},
    "embedding": {
      "timeout": {
        "name": "超时",
        "type": "integer",
        "default": 60,
        "min": 1,
        "hint": "请求超时时间（秒）"
      },
      "slow_request_threshold": {
        "name": "慢请求阈值",
        "type": "float",
        "default": 5.0,
        "min": 0,
        "hint": "超过此阈值（秒）的请求将被记录为慢请求"
      }
    }
  }
}